                                                        ._item-card-star.js-star
                                                            +icon('rating-star')
                                                        ._item-card-star.js-star
                                                            +icon('rating-star')
                                                        ._item-card-star.js-star
                                                            +icon('rating-star')
                                            ._item-source
                                                +icon('rating-yandex')
