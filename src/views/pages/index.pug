                            ._swiper-slide.swiper-slide
                                ._item.w-shadow
                                    ._item-body
                                        ._item-top
                                            ._item-card
                                                ._item-avatar
                                                    +icon('avatar-ph-female')
                                                ._item-card-box
                                                    ._item-card-name Наталья
                                                    ._item-card-stars.js-rating-stars(data-rating="1.7")
                                                        ._item-card-star.js-star
